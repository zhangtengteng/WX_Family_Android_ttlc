<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal" 
    android:background="@drawable/ysh_my_popwindow_bottom_bg">

    <LinearLayout
        android:id="@+id/pop_window"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="vertical" 
        android:background="@drawable/common_white_bg"
        android:layout_alignParentBottom="true">

        <Button
            android:id="@+id/delete_picture"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:text="删除"
            android:textColor="@color/common_title_454545_color"
            android:textSize="16sp"
            android:background="@drawable/ysh_item_bg_xml"/>
	    <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:background="@drawable/ysh_listview_driver_d3d3d3"/>
        <Button
            android:id="@+id/cancel_delete"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:layout_marginTop="1dip"
            android:background="@drawable/ysh_item_bg_xml"
            android:text="取消"
            android:textColor="@color/common_title_454545_color"
            android:textSize="16sp"/>
    </LinearLayout>

</RelativeLayout>