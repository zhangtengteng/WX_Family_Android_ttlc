<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_help_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/common_gray_bg"
    android:orientation="vertical" >

    <include layout="@layout/wx_common_title" />



    <FrameLayout
        android:id="@+id/grade_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="@drawable/common_white_bg"
        android:flipInterval="30"
        android:persistentDrawingCache="animation" ></FrameLayout>

</LinearLayout>